        "source": "https://github.com/GenCommBrasil/genpay-sdk-php"
    },
    "authors": [
        {
            "name": "Alex Silva",
            "email": "alex.silva@gencomm.com.br"
        },
        {
            "name": "GenPay",
            "email": "paytech@gencomm.com.br"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7.27"
    },
    "autoload": {
        "psr-4": {
